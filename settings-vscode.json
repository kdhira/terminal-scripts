{
    "editor.renderWhitespace": "boundary",
    "editor.showFoldingControls": "always",
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.sortOrder": "mixed",
    "files.trimFinalNewlines": true,
    "files.insertFinalNewline": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.fontFamily": "\"Roboto Mono for Powerline\"",
    "terminal.integrated.shell.osx": "/bin/zsh",

    "diffEditor.ignoreTrimWhitespace": true,
    "gitlens.defaultDateStyle": "absolute",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.configuration.checkProjectSettingsExclusions": false,
    "plantuml.fileExtensions": ".wsd,.pu,.puml,.plantuml,.iuml,.uml",
    "python.jediEnabled": false,
    "window.zoomLevel": 0,
    "[xml]": {
        "editor.defaultFormatter": "fabianlauer.vs-code-xml-format"
    },
    "java.errors.incompleteClasspath.severity": "ignore",
    "vsicons.projectDetection.disableDetect": true,
    "vsicons.dontShowNewVersionMessage": true,
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": true,
    "terminal.integrated.experimentalRefreshOnResume": true,
    "terminal.integrated.experimentalUseTitleEvent": true,
    "workbench.statusBar.visible": true,
    "problems.showCurrentInStatus": true,
    "workbench.editor.showTabs": true,
    "puppet.editorService.puppet.version": "4.7.1",
    "aws.samcli.location": "/usr/local/bin/sam",
    "aws.telemetry": false,
    "java.semanticHighlighting.enabled": true,
    "java.requirements.JDK11Warning": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
}
