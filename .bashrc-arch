# If not running interactively, don't do anything
#[[ $- != *i* ]] && return

# Import common script
[[ -f ~/.bashrc_common ]] && source ~/.bashrc_common

# Reload profile
function reloadprofile {
    source ~/.bash_profile
}

# Better ls
alias ls='ls --color=auto'
alias lsa='ls -palh'
